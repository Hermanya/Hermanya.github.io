{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "atomic-layout": "^0.7.1",
    "dotenv": "^6.2.0",
    "gatsby": "^2.1.5",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-manifest": "^2.0.18",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-source-graphql": "^2.0.10",
    "gatsby-transformer-remark": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.13",
    "gh-pages": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-feather": "^1.1.6",
    "react-helmet": "^5.2.0",
    "react-spring": "^8.0.7",
    "rebass": "^3.0.1",
    "remark": "^10.0.1",
    "remark-react": "^5.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1",
    "use-color-system": "^0.4.0",
    "use-media": "^1.3.0",
    "use-typing-effect": "^1.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "npm run build && gh-pages -d public --branch master --dotfiles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-source-github": "0.0.2",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "react/prop-types": 0,
      "capitalized-comments": 0
    }
